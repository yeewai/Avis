<h1>Listing users</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Password</th>
    <th>Role</th>
    <th>Email</th>
    <th>Notify</th>
    <th>Gmt</th>
    <th>Status</th>
    <th>Name</th>
    <th>Birthday</th>
    <th>Zodiac</th>
    <th>Residence</th>
    <th>Colour</th>
    <th>Position</th>
    <th>Website</th>
    <th>Bio</th>
    <th>Quote</th>
    <th>Contact</th>
    <th>Theme</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.password_hash %></td>
    <td><%= user.role %></td>
    <td><%= user.email %></td>
    <td><%= user.notify %></td>
    <td><%= user.gmt %></td>
    <td><%= user.status %></td>
    <td><%= user.name %></td>
    <td><%= user.birthday %></td>
    <td><%= user.zodiac %></td>
    <td><%= user.residence %></td>
    <td><%= user.colour %></td>
    <td><%= user.position %></td>
    <td><%= user.website %></td>
    <td><%= user.bio %></td>
    <td><%= user.quote %></td>
    <td><%= user.contact %></td>
    <td><%= user.theme %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
